{
  "name": "react-redux-base",
  "version": "1.0.0",
  "description": "Base setup for React / Redux apps",
  "scripts": {
    "build:webpack": "NODE_ENV=production webpack --config webpack.config.prod.js",
    "build:dev": "webpack --config webpack.config.dev.js",
    "start": "npm run build:dev && node devServer.js",
    "test": "env NODE_PATH=$NODE_PATH:$PWD/assets/js mocha ./assets/js/tests/ --compilers js:babel-core/register --require ./assets/js/tests/test_helper.js --recursive",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint assets"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joshaddington/react-redux-base.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "webpack",
    "babel",
    "react-transform"
  ],
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.3.15",
    "babel-eslint": "^5.0.0-beta4",
    "babel-loader": "^6.2.0",
    "babel-plugin-react-transform": "^2.0.0-beta1",
    "babel-plugin-syntax-object-rest-spread": "^6.3.13",
    "babel-plugin-transform-object-rest-spread": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.4.1",
    "chai-immutable": "^1.5.3",
    "css-loader": "^0.23.0",
    "enzyme": "^1.3.0",
    "eslint": "^1.10.3",
    "eslint-plugin-babel": "^3.0.0",
    "eslint-plugin-react": "^3.11.3",
    "exports-loader": "^0.6.2",
    "express": "^4.13.3",
    "history": "^1.13.1",
    "immutable": "^3.7.6",
    "imports-loader": "^0.6.5",
    "mocha": "^2.3.4",
    "moment": "^2.11.1",
    "nock": "^5.2.1",
    "react-addons-test-utils": "^0.14.6",
    "react-transform-catch-errors": "^1.0.0",
    "react-transform-hmr": "^1.0.1",
    "redbox-react": "^1.0.1",
    "redux-logger": "^2.5.2",
    "redux-mock-store": "0.0.6",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "unexpected": "^10.8.0",
    "unexpected-react": "^0.5.1",
    "webpack": "^1.12.9",
    "webpack-bundle-tracker": "0.0.93",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "isomorphic-fetch": "^2.2.1",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.1",
    "react-router": "^1.0.3",
    "redux": "^3.0.5",
    "redux-router": "^1.0.0-beta7",
    "redux-thunk": "^1.0.2"
  }
}
